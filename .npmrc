
# Enforce npm as the only package manager
engine-strict=true
package-lock=true
fund=false
audit=false
save-exact=false
legacy-peer-deps=false

# Explicitly disable other package managers
enable-pre-post-scripts=true
ignore-scripts=false

# Force npm as the package manager
package-manager-strict=true

# Disable corepack completely
corepack=false

# Additional npm enforcement
prefer-offline=false
prefer-online=true
