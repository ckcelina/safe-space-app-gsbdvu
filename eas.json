{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "node": "18.0.0",
      "npm": {
        "install": "npm ci"
      },
      "env": {
        "COREPACK_ENABLE_STRICT": "0",
        "COREPACK_ENABLE_AUTO_PIN": "0",
        "DISABLE_COREPACK": "1",
        "npm_config_user_agent": "npm",
        "npm_config_package_manager": "npm",
        "NPM_CONFIG_FUND": "false",
        "NPM_CONFIG_AUDIT": "false"
      }
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "node": "18.0.0",
      "npm": {
        "install": "npm ci"
      },
      "env": {
        "COREPACK_ENABLE_STRICT": "0",
        "COREPACK_ENABLE_AUTO_PIN": "0",
        "DISABLE_COREPACK": "1",
        "npm_config_user_agent": "npm",
        "npm_config_package_manager": "npm",
        "NPM_CONFIG_FUND": "false",
        "NPM_CONFIG_AUDIT": "false"
      }
    },
    "production": {
      "autoIncrement": true,
      "node": "18.0.0",
      "npm": {
        "install": "npm ci"
      },
      "env": {
        "COREPACK_ENABLE_STRICT": "0",
        "COREPACK_ENABLE_AUTO_PIN": "0",
        "DISABLE_COREPACK": "1",
        "npm_config_user_agent": "npm",
        "npm_config_package_manager": "npm",
        "NPM_CONFIG_FUND": "false",
        "NPM_CONFIG_AUDIT": "false"
      }
    }
  }
}