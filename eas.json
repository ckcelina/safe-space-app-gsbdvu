
{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true,
      "node": "18.0.0",
      "npm": {
        "install": "npm ci"
      },
      "env": {
        "COREPACK_ENABLE_STRICT": "0",
        "COREPACK_ENABLE_AUTO_PIN": "0",
        "npm_config_user_agent": "npm",
        "DISABLE_COREPACK": "1",
        "npm_execpath": "/usr/local/bin/npm",
        "npm_config_package_manager": "npm"
      },
      "prebuildCommand": "corepack disable || true"
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "node": "18.0.0",
      "npm": {
        "install": "npm ci"
      },
      "env": {
        "COREPACK_ENABLE_STRICT": "0",
        "COREPACK_ENABLE_AUTO_PIN": "0",
        "npm_config_user_agent": "npm",
        "DISABLE_COREPACK": "1",
        "npm_execpath": "/usr/local/bin/npm",
        "npm_config_package_manager": "npm"
      },
      "prebuildCommand": "corepack disable || true"
    },
    "production": {
      "autoIncrement": true,
      "node": "18.0.0",
      "npm": {
        "install": "npm ci"
      },
      "env": {
        "COREPACK_ENABLE_STRICT": "0",
        "COREPACK_ENABLE_AUTO_PIN": "0",
        "npm_config_user_agent": "npm",
        "DISABLE_COREPACK": "1",
        "npm_execpath": "/usr/local/bin/npm",
        "npm_config_package_manager": "npm"
      },
      "prebuildCommand": "corepack disable || true"
    }
  }
}
